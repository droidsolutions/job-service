<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" DefaultTargets="Build"
  xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <!-- Can be used when compile fails to disable TypeScript generation temporarily -->
    <RtDisable>false</RtDisable>
    <RtDivideTypesAmongFiles>true</RtDivideTypesAmongFiles>
    <RtTargetDirectory>$(ProjectDir)../NodeJS/Generated</RtTargetDirectory>
    <CamelCaseForProperties>true</CamelCaseForProperties>
  </PropertyGroup>
</Project>